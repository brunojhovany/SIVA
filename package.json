        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "resolve-url-loader": "2.3.1",
        "sass": "^1.15.2",
        "sass-loader": "7.*",
        "vue": "^2.5.7",
        "vue-template-compiler": "^2.5.21"
    },
    "dependencies": {
        "material-design-icons": "^3.0.1",
        "materialize-css": "^1.0.0-rc.2",
        "npm": "^6.4.1",
        "sweetalert2": "^7.26.11"
    }
